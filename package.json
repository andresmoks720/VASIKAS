{
  "name": "vasikas",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mock-api": "npm run dev --prefix tools/mock-api",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:mock-api\"",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "gen:mocks": "tsx scripts/generate-mocks.ts",
    "test:e2e": "playwright test",
    "eaip:pull": "npm run eaip:pull --prefix tools/eaip-import"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "cheerio": "^1.1.2",
    "ol": "^10.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^1.6.1",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "express": "^5.2.1",
    "jsdom": "^24.1.0",
    "msw": "^2.12.4",
    "playwright": "^1.45.0",
    "supertest": "^7.1.4",
    "tsx": "^4.21.0",
    "typescript": "5.5.4",
    "vite": "^5.4.0",
    "vitest": "^1.6.0"
  }
}
